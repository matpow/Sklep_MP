
<html>

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <title>Electro - Sport store</title>

    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,700" rel="stylesheet">

    <!-- Bootstrap -->
    <link type="text/css" rel="stylesheet" href="~/Content/Template_electro/electro/bootstrap.min.css" />

    <!-- Slick -->
    <link type="text/css" rel="stylesheet" href="~/Content/Template_electro/electro/slick.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/Template_electro/electro/slick-theme.css" />

    <!-- nouislider -->
    <link type="text/css" rel="stylesheet" href="~/Content/Template_electro/electro/nouislider.min.css" />

    <!-- Font Awesome Icon -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

    <!-- Custom stlylesheet -->
    <link type="text/css" rel="stylesheet" href="~/Content/Template_electro/electro/style.css" />


</head>


<body>
    <!-- HEADER -->
    <header>
        <!-- TOP HEADER -->
        <div id="top-header">
            <div class="container">
                <ul class="header-links pull-left">
                    <li><a href="#"><i class="fa fa-phone"></i> 605-000-99</a></li>
                    <li><a href="#"><i class="fa fa-envelope-o"></i> kleos13@wp.pl</a></li>
                    <li><a href="#"><i class="fa fa-map-marker"></i> Warszawa, Unnamed 12a</a></li>
                </ul>
                <ul class="header-links pull-right">
                    <li><a href="#"><i class="fa fa-dollar"></i> zł</a></li>
                    <li><a href="#"><i class="fa fa-user" aria-hidden="true"></i> Moje konto</a></li>
                </ul>
            </div>
        </div>
        <!-- /TOP HEADER -->
        <!-- MAIN HEADER -->
        <div id="header">
            <!-- container -->
            <div class="container">
                <!-- row -->
                <div class="row">
                    <!-- LOGO -->
                    <div class="col-md-3">
                        <div class="header-logo">
                            <a class="logo" href=@Url.Action("Index","Home")>
                                <img src="~/Content/Template_electro/electro/Images/logo.png" alt="">
                            </a>
                        </div>
                    </div>
                    <!-- /LOGO -->
                    <!-- SEARCH BAR -->
                    <div class="col-md-6">
                        <div class="header-search">
                            <form>
                                <select class="input-select">
                                    <option value="0">All Categories</option>
                                    <option value="1">Category 01</option>
                                    <option value="1">Category 02</option>
                                </select>
                                <input class="input" id="search-filter" type="search" name="searchQuery"
                                       data-autocomplete-source="@Url.Action("ProduktyPodpowiedzi")"
                                       placeholder="Wyszukaj..." />

                                <button class="search-btn" type="submit" value="">Szukaj</button>
                            </form>
                        </div>
                    </div>
                    <!-- /SEARCH BAR -->
                    <!-- ACCOUNT -->
                    <div class="col-md-3 clearfix">
                        <div class="header-ctn">
                            <!-- Wishlist -->
                            <div>
                                <a href="#">
                                    <i class="fa fa-heart-o"></i>
                                    <span>Koszyk</span>
                                    <div class="qty">2</div>
                                </a>
                            </div>
                            <!-- /Wishlist -->
                            <!-- Cart -->
                            <div class="dropdown">
                                <a class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">
                                    <i class="fa fa-shopping-cart"></i>
                                    <span>Your Cart</span>
                                    <div class="qty">@Html.Action("PobierzIloscElementowKoszyka", "Koszyk")</div>
                                </a>
                                <div class="cart-dropdown">
                                    <div class="cart-list">
                                        

                                    </div>
                                    <div class="cart-summary">
                                        <small>@Html.Action("PobierzIloscElementowKoszyka","Koszyk")   Item(s) selected</small>
                                        <h5>SUBTOTAL: @Html.Action("PobierzCeneCalkowita","Koszyk") zł</h5>
                                    </div>
                                    <div class="cart-btns">
                                        <a href="#">View Cart</a>
                                        <a href=@Url.Action("Koszyk","Koszyk") >Checkout  <i class="fa fa-arrow-circle-right"></i></a>
                                    </div>
                                </div>
                            </div>
                            <!-- /Cart -->
                            <!-- Menu Toogle -->
                            <div class="menu-toggle">
                                <a href="#">
                                    <i class="fa fa-bars"></i>
                                    <span>Menu</span>
                                </a>
                            </div>
                            <!-- /Menu Toogle -->
                        </div>
                    </div>
                    <!-- /ACCOUNT -->
                </div>
                <!-- row -->
            </div>
            <!-- container -->
        </div>
        <!-- /MAIN HEADER -->
    </header>
    <!-- /HEADER -->
    <!-- NAVIGATION -->
    <nav id="navigation">
        <!-- container -->
        <div class="container">
            <!-- responsive-nav -->
            <div id="responsive-nav">

                <!-- NAV -->
                <!-- /NAV -->

            </div>
            <!-- /responsive-nav -->
        </div>
        <!-- /container -->
    </nav>
    <!-- /NAVIGATION -->

    <div>
        @RenderBody()
    </div>


    <!-- FOOTER -->
    <footer id="footer">
        <!-- top footer -->
        <div class="section">
            <!-- container -->
            <div class="container">
                <!-- row -->
                <div class="row">
                    <div class="col-md-3 col-xs-6">
                        <div class="footer">
                            <h3 class="footer-title">O nas</h3>
                            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut.</p>
                            <ul class="footer-links">
                                <li><a href="#"><i class="fa fa-map-marker"></i>Warszawa, Unnamed 12a</a></li>
                                <li><a href="#"><i class="fa fa-phone"></i>605-000-99</a></li>
                                <li><a href="#"><i class="fa fa-envelope-o"></i>email@email.com</a></li>
                            </ul>
                        </div>
                    </div>

                    <div class="col-md-3 col-xs-6">
                        <div class="footer">
                            <h3 class="footer-title">Kategorie</h3>
                            <ul class="footer-links">
                                <li><a href="#">Hot deals</a></li>
                                <li><a href="#">Sporty druzynowe</a></li>
                                <li><a href="#">Odziez</a></li>
                                <li><a href="#">Obuwie</a></li>
                                <li><a href="#">Sprzet</a></li>
                            </ul>
                        </div>
                    </div>

                    <div class="clearfix visible-xs"></div>

                    <div class="col-md-3 col-xs-6">
                        <div class="footer">
                            <h3 class="footer-title">Informacje</h3>
                            <ul class="footer-links">
                                <li>@Html.ActionLink("O nas", "StronyStatyczne", "Home", new { nazwa = "Onas" }, null) </li>
                                <li>@Html.ActionLink("Kontakt", "StronyStatyczne", "Home", new { nazwa = "Kontakt" }, null)</li>
                                <li>@Html.ActionLink("Polityka prywatnosci", "StronyStatyczne", "Home", new { nazwa = "PolitykaPrywatnosci" }, null)</li>
                                <li>@Html.ActionLink("Zamowienia i zwroty", "StronyStatyczne", "Home", new { nazwa = "ZamowieniaZwroty" }, null)</li>
                                <li>@Html.ActionLink("Warunki", "StronyStatyczne", "Home", new { nazwa = "Warunki" }, null)</li>
                            </ul>
                        </div>
                    </div>

                    <div class="col-md-3 col-xs-6">
                        <div class="footer">
                            <h3 class="footer-title">Uslugi</h3>
                            <ul class="footer-links">
                                <li><a href="#">Moje konto</a></li>
                                <li><a href="#">Koszyk</a></li>
                                <li><a href="#">Lista</a></li>
                                <li><a href="#">Sledz zamowienie</a></li>
                                <li><a href="#">Pomoc</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <!-- /row -->
            </div>
            <!-- /container -->
        </div>
        <!-- /top footer -->
        <!-- bottom footer -->
        <div id="bottom-footer" class="section">
            <div class="container">
                <!-- row -->
                <div class="row">
                    <div class="col-md-12 text-center">
                        <ul class="footer-payments">
                            <li><a href="#"><i class="fa fa-cc-visa"></i></a></li>
                            <li><a href="#"><i class="fa fa-credit-card"></i></a></li>
                            <li><a href="#"><i class="fa fa-cc-paypal"></i></a></li>
                            <li><a href="#"><i class="fa fa-cc-mastercard"></i></a></li>
                            <li><a href="#"><i class="fa fa-cc-discover"></i></a></li>
                            <li><a href="#"><i class="fa fa-cc-amex"></i></a></li>
                        </ul>
                        <span class="copyright">
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;
                            <script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        </span>
                    </div>
                </div>
                <!-- /row -->
            </div>
            <!-- /container -->
        </div>
        <!-- /bottom footer -->
    </footer>
    <!-- /FOOTER -->
    @RenderSection("Scripts", required: false)
    <!-- jQuery Plugins -->
    <script src="js/jquery.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/slick.min.js"></script>
    <script src="js/nouislider.min.js"></script>
    <script src="js/jquery.zoom.min.js"></script>
    <script src="js/main.js"></script>
    <script src="js/jquery-ui-1.12.1.js"></script>
    <script src="js/jquery-ui-1.12.1.min.js"></script>

    @section Scritpts
        {
        @System.Web.Optimization.Scripts.Render("~/bundles/jqueryAndJqueryUI");
    }

    <script>
        $(function () {
            var setAutoComplete = function () {
                var $input = $(this);

                var options =
                    {
                        source: $input.attr("data-autocomplete-source"),
                        select: function (event, ui) {
                            $input = $(this);
                            $input.val(ui.item.label);
                            var $form = $input.parents("form:first");
                            $form.submit();
                        }
                    };
                $input.autocomplete(options);
            };

            $("#search-filter").each(setAutoComplete);
        });
    </script>

</body>
</html>